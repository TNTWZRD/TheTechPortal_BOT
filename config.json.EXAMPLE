    "MYSQL":{
      "HOST":"MYSQLHOST",
      "DB":"MYSQL DATABASE",
      "USER":"MYSQL USER",
      "PASSWORD":"MYSQL PASSWORD"
    },
    # Module Bit For all servers, DO NOT TOUCH UNLESS YOU KNOW WHAT YOU ARE DOING
    "MODULES":{
      "SYSTEM": 0,
      "MODERATION": 1,
      "GITHUB": 2,
      "FUN": 4,
      "DND": 8,
      "MUSIC": 16
    },
