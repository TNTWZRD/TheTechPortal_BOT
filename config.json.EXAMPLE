{
    # Default Prefix when none specified
    "prefix":"!",
    # Your Bot's TOKEN
    "token":"Your_Token",
    # Your Github Personal Auth Token and Repoitory if you would like users to use `!gitissue`
    "github_oauth":"[Your GithubOAUTH TOKEN]",
    "github_repository":"https://api.github.com/repos/<USER>/<REPOS>/issues",
    # Permission Bits for all servers, DO NOT TOUCH UNLESS YOU KNOW WHAT YOU ARE DOING
